<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Images</title>
</head>
<body>

<h1>Here are your images</h1>
{{range .}}
<img src="/public/pics/{{.}}" width="400px">
{{end}}

<br>
<h1>Upload some images</h1>
<form method="POST" enctype="multipart/form-data">
    <label for="idx-f">Choose File To Upload</label>
    <input type="file" id="idx-f" name="nf">
    <br>
    <input type="submit">
</form>

</body>
</html>
